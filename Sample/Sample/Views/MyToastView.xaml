    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    xmlns:extra="clr-namespace:AiForms.Extras.Abstractions;assembly=AiForms.Extras"
    x:Class="Sample.Views.MyToastView">
    <Label Text="{Binding Message}" TextColor="White" FontSize="14"
           HorizontalTextAlignment="Center"
           VerticalTextAlignment="Center" />
</extra:ToastView>
